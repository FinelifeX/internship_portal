{
  "parser": "babel-eslint",
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "plugins": ["prettier"],
  "ignorePatterns": ["*.d.ts"],
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["node_modules", "modules"]
      }
    }
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-props-no-spreading": ["error", { "custom": "ignore" }]
  }
}
